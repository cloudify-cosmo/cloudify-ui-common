{
    "extends": ["plugin:jsdoc/recommended", "./configs/eslint-common-node.json"],
    "rules": {
        "jsdoc/require-jsdoc": [
            "error",
            {
                "publicOnly": true
            }
        ]
    },
    "settings": {
        "jsdoc": {
            "preferredTypes": {
                "object": "Object"
            }
        }
    }
}
