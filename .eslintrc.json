{
    "extends": [
        "airbnb-base",
        "plugin:jsdoc/recommended",
        "./eslint/eslint-common.json"
    ],
    "plugins": [
        "jsdoc"
    ],
    "rules": {
        "jsdoc/require-jsdoc": ["error"]
    },
    "overrides": [
        {
            "files": "test/**/*.js",
            "env": {
                "jest": true
            }
        }
    ]
}
