{
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:jsdoc/recommended"
    ],
    "plugins": [
        "prettier",
        "jsdoc"
    ],
    "rules": {
        "jsdoc/require-jsdoc": ["error"],
        "prettier/prettier": ["error"]
    },
    "overrides": [
        {
            "files": "test/**/*.js",
            "env": {
                "jest": true
            }
        }
    ]
}
