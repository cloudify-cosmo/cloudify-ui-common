{
    "extends": ["plugin:jsdoc/recommended", "./configs/eslint-common.json"],
    "rules": {
        "react/jsx-props-no-spreading": "off",
        "no-void": "off",
        "@typescript-eslint/no-non-null-assertion": "off",
        "jsdoc/require-jsdoc": [
            "error",
            {
                "publicOnly": true
            }
        ]
    },
    "settings": {
        "jsdoc": {
            "preferredTypes": {
                "object": "Object"
            }
        }
    },
    "overrides": [
        {
            "files": ["*.tsx"],
            "rules": {
                "react/prop-types": "off"
            }
        }
    ]
}
