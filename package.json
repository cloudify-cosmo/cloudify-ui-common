{
    "name": "cloudify-ui-common",
    "version": "7.3.2",
    "description": "Common Cloudify UI library",
    "homepage": "https://github.com/cloudify-cosmo/cloudify-ui-common#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cloudify-cosmo/cloudify-ui-common.git"
    },
    "license": "Apache-2.0",
    "author": "Cloudify Platform Ltd. <cosmo-admin@cloudify.co>",
    "types": "./typings/index.d.ts",
    "files": [
        "backend/lib",
        "backend/typings",
        "backend/package.json",
        "backend/*.ts",
        "cypress",
        "dist",
        "configs",
        "fonts",
        "images",
        "scripts",
        "src",
        "styles",
        "typings"
    ],
    "main": "dist/cloudify-ui-common.cjs.js",
    "module": "dist/cloudify-ui-common.esm.js",
    "browser": "dist/cloudify-ui-common.min.js",
    "scripts": {
        "audit": "npm audit --production",
        "build": "npm run build:code && npm run generate-type-declarations && npm run compile-ts",
        "build:code": "rollup -c",
        "check-types": "npm run check-types:frontend && npm run check-types:backend",
        "check-types:frontend": "tsc -p src --noEmit --emitDeclarationOnly false && tsc -p test --noEmit && tsc -p cypress --noEmit",
        "check-types:backend": "tsc -p backend --noEmit --emitDeclarationOnly false && tsc -p backend/test --noEmit",
        "compile-ts": "npm run compile-ts:frontend && npm run compile-ts:backend",
        "compile-ts:frontend": "tsc -p src && tsc -p cypress",
        "compile-ts:backend": "tsc -p backend",
        "dev": "rollup -c -w",
        "generate-type-declarations": "tsc -p src && tsc -p backend",
        "lint": "eslint --cache --ignore-path .gitignore .",
        "publish:major": "bash ci/publish-version.sh major",
        "publish:minor": "bash ci/publish-version.sh minor",
        "publish:patch": "bash ci/publish-version.sh patch",
        "test": "npm run lint && npm run test:coverage",
        "test:coverage": "jest test --coverage",
        "test:only": "jest test"
    },
    "dependencies": {
        "axios": "^0.26.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^6.12.0",
        "lodash": "^4.17.21",
        "sequelize": "^6.21.3",
        "umzug": "^3.0.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.12.7",
        "@cypress/code-coverage": "^3.10.0",
        "@cypress/webpack-dev-server": "^1.6.0",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.1.0",
        "@rollup/plugin-node-resolve": "^11.1.1",
        "@types/jest": "^29.0.0",
        "@types/lodash": "^4.14.170",
        "@types/node": "^12.19.16",
        "@types/sequelize": "^4.28.10",
        "@types/tmp": "^0.2.1",
        "@types/umzug": "^2.3.3",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-jest": "^26.5.2",
        "browserslist": "^4.21.1",
        "cypress": "^8.2.0",
        "eslint": "^7.32.0",
        "eslint-plugin-chai-friendly": "^0.6.0",
        "eslint-plugin-cypress": "^2.11.3",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.0",
        "eslint-plugin-jsdoc": "^30.6.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-scanjs-rules": "^0.2.1",
        "eslint-plugin-security": "^1.4.0",
        "jest": "^29.0.2",
        "prettier": "^2.6.2",
        "rollup": "^2.38.1",
        "rollup-plugin-terser": "^7.0.2",
        "tmp": "^0.2.1",
        "typescript": "~4.4.4"
    },
    "peerDependencies": {
        "@cypress/code-coverage": "^3.10.0",
        "@cypress/webpack-dev-server": "^1.6.0",
        "@typescript-eslint/eslint-plugin": ">=4.14.1",
        "@typescript-eslint/parser": ">=4.14.1",
        "cypress": "^8.2.0",
        "eslint": "^7.11.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-chai-friendly": "^0.6.0",
        "eslint-plugin-cypress": "^2.11.3",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.21.4",
        "eslint-plugin-react-hooks": "^4.1.2",
        "eslint-plugin-scanjs-rules": "^0.2.1",
        "eslint-plugin-security": "^1.4.0"
    },
    "engines": {
        "node": ">=16"
    }
}
