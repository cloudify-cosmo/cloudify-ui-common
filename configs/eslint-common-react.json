{
    "extends": ["airbnb", "prettier/react", "plugin:prettier/recommended", "./eslint-common.json"],
    "rules": {
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "react/jsx-filename-extension": ["error", { "extensions": [".jsx", ".tsx"] }]
    },
    "env": {
        "browser": true
    }
}
